{
  "name": "said-sdk",
  "version": "0.3.3",
  "description": "SDK for SAID Protocol - Identity infrastructure for AI agents on Solana",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "said": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/cli.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/cli.ts --format cjs,esm --dts --watch",
    "test": "node test.mjs",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "solana",
    "ai-agents",
    "identity",
    "said",
    "verification",
    "reputation",
    "cli"
  ],
  "author": "Kai <@kaiclawd>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kaiclawd/said-sdk"
  },
  "homepage": "https://saidprotocol.com",
  "dependencies": {
    "@solana/web3.js": "^1.98.0",
    "bs58": "^5.0.0",
    "commander": "^12.1.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
